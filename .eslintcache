e.jsx",["83"],"import { useState, useEffect, useRef } from 'react';\nimport * as itemsAPI from '../../utilities/items-api';\nimport * as ordersAPI from '../../utilities/orders-api';\n// Add the component imports\nimport './NewOrderPage.css';\nimport { Link } from 'react-router-dom';\nimport Logo from '../../components/Logo/Logo';\nimport MenuList from '../../components/MenuList/MenuList';\nimport CategoryList from '../../components/CategoryList/CategoryList';\nimport OrderDetail from '../../components/OrderDetail/OrderDetail';\nimport UserLogOut from '../../components/UserLogOut/UserLogOut';\n\nexport default function NewOrderPage({ user, setUser }) {\n\tconst [menuItems, setMenuItems] = useState([]);\n\t// Add state to track the \"active\" category\n\tconst [activeCat, setActiveCat] = useState('');\n\tconst [cart, setCart] = useState(null);\n\n\t// Create and initialize the ref to an empty array\n\tconst categoriesRef = useRef([]);\n\n\tuseEffect(() => {\n\t\tasync function getItems() {\n\t\t\tconst items = await itemsAPI.getAll();\n\t\t\tcategoriesRef.current = items.reduce((cats, item) => {\n\t\t\t\tconst cat = item.category.name;\n\t\t\t\treturn cats.includes(cat) ? cats : [...cats, cat];\n\t\t\t}, []);\n\t\t\tsetMenuItems(items);\n\t\t\t// Add this line to initialize the active category\n\t\t\tsetActiveCat(items[0].category.name);\n\t\t}\n\t\tgetItems();\n\n\t\t// Load cart (a cart is the unpaid order for the logged in user)\n\t\tasync function getCart() {\n\t\t\tconst cart = await ordersAPI.getCart();\n\t\t\tsetCart(cart);\n\t\t}\n\t\tgetCart();\n\t}, []);\n\n\treturn (\n\t\t<main className='NewOrderPage'>\n\t\t\t<aside>\n\t\t\t\t<Logo />\n\t\t\t\t<CategoryList\n\t\t\t\t\tcategories={categoriesRef.current}\n\t\t\t\t\tactiveCat={activeCat}\n\t\t\t\t\tsetActiveCat={setActiveCat}\n\t\t\t\t/>\n\t\t\t\t<Link to='/orders' className='button btn-sm'>\n\t\t\t\t\tPREVIOUS ORDERS\n\t\t\t\t</Link>\n\t\t\t\t<UserLogOut user={user} setUser={setUser} />\n\t\t\t</aside>\n\t\t\t<MenuList\n\t\t\t\tmenuItems={menuItems.filter(\n\t\t\t\t\titem => item.category.name === activeCat\n\t\t\t\t)}\n\t\t\t/>\n\t\t\t<OrderDetail />\n\t\t</main>\n\t);\n}\n","/Users/Daniel/general-assembly/sei-cafe/src/utilities/users-api.js",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/Logo/Logo.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/SignUpForm/SignUpForm.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/LoginForm/LoginForm.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/utilities/items-api.js",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/MenuList/MenuList.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/CategoryList/CategoryList.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/OrderDetail/OrderDetail.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/UserLogOut/UserLogOut.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/MenuListItem/MenuListItem.jsx",[],"/Users/Daniel/general-assembly/sei-cafe/src/utilities/send-request.js",[],"/Users/Daniel/general-assembly/sei-cafe/src/components/LineItem/LineItem.jsx",[],["84","85"],"/Users/Daniel/general-assembly/sei-cafe/src/utilities/orders-api.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":17,"column":9,"nodeType":"92","messageId":"93","endLine":17,"endColumn":13},{"ruleId":"86","replacedBy":"94"},{"ruleId":"88","replacedBy":"95"},"no-native-reassign",["96"],"no-negated-in-lhs",["97"],"no-unused-vars","'cart' is assigned a value but never used.","Identifier","unusedVar",["96"],["97"],"no-global-assign","no-unsafe-negation"]